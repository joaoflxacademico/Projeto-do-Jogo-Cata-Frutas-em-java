package cata_frutas.Elementos.Elementos_Din√¢micos;

public class Coco extends Fruta{
	
	public Coco(int x, int y, float bichada) {
		super(x,y,bichada);
		this.nome = "Coco";
	}
	
	@Override
 	public void efeito(Jogador jogador) {
		jogador.movimentosRestantes = jogador.movimentosRestantes * 2;
	}
}
